        <Class name="org.jsimpledb.vaadin.app.MainPanel"/>
        <Bug pattern="SE_BAD_FIELD"/>
    </Match>
    <Match>
        <Class name="org.jsimpledb.vaadin.app.MainPanel"/>
        <Bug pattern="SE_BAD_FIELD_STORE"/>
    </Match>
    <Match>
        <Class name="org.jsimpledb.vaadin.app.MainPanel"/>
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>
    <Match>
        <Class name="org.jsimpledb.vaadin.app.ReferenceFieldField"/>
